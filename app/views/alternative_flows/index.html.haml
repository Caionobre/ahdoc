= list_title

%table.records
  %tr
    %th 
      = resource_class.human_attribute_name :title
    %th

  - collection.each do |resource|
    %tr>
      %td
        = resource.title
      %td
        = edit_link_to resource
        = destroy_link_to resource
        = link_to Item.model_name.human, alternative_flow_items_path(resource)

%br 

= new_link_to
= up_link_to
